@{
    ViewBag.Title = "Index tenant ";
}


<div class="panel-body">
    <table class="table">
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Comprador
            </th>
            <th>
                Vendedor
            </th>
        </tr>
        @foreach (var item in ViewBag.ListarSubastas)
        {
            <tr class="texto_table">
                <td>
                    @item.id
                </td>
                <td>
                    @item.id_Comprador
                </td>
                <td>
                    @item.id_Vendedor
                </td>
            </tr>
        }
    </table>

    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>

</div>







<h1>@ViewBag.Message</h1>
